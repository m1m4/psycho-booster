{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env -- next dev -H 0.0.0.0 --webpack",
    "build": "dotenv -e ../../.env -- next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "export": "dotenv -e ../../.env -- next build --webpack",
